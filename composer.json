{
    "name": "darxide-pl/cakephp-filter",
    "description": "Filtering Queries plugin with extended FormHelper for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Filter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Filter\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
